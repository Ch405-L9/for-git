{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "PreCheckReport",
  "type": "object",
  "properties": {
    "shn": {
      "type": "string"
    },
    "status": {
      "type": "string",
      "enum": [
        "pass",
        "fail"
      ]
    },
    "checks": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "status": {
            "type": "string",
            "enum": [
              "pass",
              "fail"
            ]
          },
          "details": {
            "type": "string"
          }
        },
        "required": [
          "name",
          "status"
        ]
      }
    }
  },
  "required": [
    "shn",
    "status",
    "checks"
  ]
}